  <%- include('partials/logheader') %>


<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    
    <hr>
    
  </div>
  <div class="box">
    
    <div class="box">
    
      
        
    
        <div class="item">

        
  
        </div>
 
  
        
    </div>
  
  </div>
  <%projects.forEach(function(item){%>

  <div class="p-10">  
    <!--Card 1-->
    <div class="max-w-sm rounded overflow-hidden shadow-lg">
        <p><%=  item.name  %></p>

      <img class="w-full"  src="data:image/<%=item.image.contentType%>;base64,
              <%=item.image.data.toString('base64')%>" style="width:40%;align-items: center;">
      <div class="px-6 py-4">
        <div class="font-bold text-xl mb-2"> <p><%=  item.domain  %></p></div>
        <p class="text-gray-700 text-base">
          <p><%=  item.desc  %></p>        </p>
      </div>
      <div class="px-6 pt-4 pb-2">
        <span class="inline-block bg-gray-200 rounded-full px-3 py-1 text-sm font-semibold text-gray-700 mr-2 mb-2">#<%=item.technology  %></span>
    
      </div>
    </div>
  </div>
  <% }) %>

</div>

